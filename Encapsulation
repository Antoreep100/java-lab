class Person
{
 private String name;
 private int id;
 
  public Person(String name, int id) 
  {
        this.name = name;
        this.id = id;
  }
 public String getName()
 
 {
   return name;
 }
 public void setName(String name)
 {
   this.name=name;
 }
 public int getId()
 {
   return id;
 }
 public void setId(int id)
 {
   this.id=id;
 }
 void display()
 {
   System.out.println("Name: " + name + " Id" + id);
 }
 public static void main(String[] args)
 {
   Person s1 =new Person("John",129);
   s1.display();
  
   
 }
}
